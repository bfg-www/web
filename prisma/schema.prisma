// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Nea {
  brand             String
  model             String @id
  greenTicks        Int
  annualConsumption Int
}

model Sale {
  model             String @id
  price             Int
  image             String
  url               String
  description       String
}

model Aircon {
  id                Int @id @default(autoincrement())
  brand             String
  model             String @unique
  greenTicks        Int
  annualConsumption Int
  price             Int
  image             String
  url               String
  description       String
}
